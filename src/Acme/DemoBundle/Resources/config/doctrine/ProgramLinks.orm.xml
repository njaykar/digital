<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\DemoBundle\Entity\ProgramLinks" table="program_links">
    <id name="name" type="string" column="name" length="50">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="url" type="string" column="url" length="200" nullable="false"/>
    <field name="type" type="string" column="type" length="50" nullable="false"/>
    <field name="desc" type="string" column="desc" length="200" nullable="false"/>
    <field name="rating" type="integer" column="rating" nullable="false"/>
  </entity>
</doctrine-mapping>
